@model CheckOut.Models.ToDo

<h2>Create ToDo</h2>

<form method="post">
    <div>
        <label for="ToDoId">ToDoId</label>
        <input type="int" id="ToDoId" name="ToDoId" value="@Model.ToDoId" required />
    </div>
    <div>
        <label for="Description">Description</label>
        <input type="text" id="Description" name="Description" value="@Model.Description" required />
    </div>
    <button type="submit">Create</button>
</form>